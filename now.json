{
  "name": "express-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],

  "env": {
    "DB_USER": "@th_db_user",
    "DB_PASSWORD": "@th_db_password",
    "DB_HOST": "@th_db_host",
    "DB_NAME": "@th_db_name",
    "DB_PORT": "@th_db_port",
    "DEFAULT_ADMIN_PASSWORD": "@th_default_admin_password",
    "DEFAULT_USER_PASSWORD": "@th_default_user_password",
    "AUTH_JWT_SECRET": "@th_auth_jwt_secret",
    "PUBLIC_API_KEY_TOKEN": "@th_public_api_key_token",
    "ADMIN_API_KEY_TOKEN": "@th_admin_api_key_token",
    "GCS_BUCKET":"@th_gcs_bucket",
    "GCLOUD_PROJECT":"@th_gcloud_project",
    "GCS_KEYFILE":"@th_gcs_keyfile",
    "SPOTIFY_CLIENT_ID": "@th_spotify_client_id",
    "SPOTIFY_CLIENT_SECRET": "@th_spotify_client_secret",
    "GOOGLE_CLIENT_ID": "@th_google_client_id",
    "GOOGLE_CLIENT_SECRET": "@th_google_client_secret"
  }
}




